#!/bin/ksh
# Ignore HUP, INT, QUIT now.
trap "" 1 3 15

#---------------------------------------------------------------------
# ENVIRONMENT
#
HOME_DIR=/hw01/ibridge
HOST_NAME=`hostname`
IBDIR=${HOME_DIR}/ib2
IBBINDIR=${HOME_DIR}/ib2/bin

#---------------------------------------------------------------------
# FUNCTION     killSvc
#     ex) ps -ef | grep OIRKBB0101 | grep -v vi | grep -v tail | grep -v grep | grep -v xrun | grep -v xend
#         ps -ef | grep OIRKBB0101 | grep -v vi | grep -v tail | grep -v grep | grep -v xrun | grep -v xend | awk '{ print $2 }'
#
killSvc()
{
	# echo "STATUS : ¼­ºñ½º(${SVC_NM}:${SVC_CD})À» Á¾·áÇÕ´Ï´Ù..."

	PID=`ps -ef | grep ${SVC_NM} | grep -v vi | grep -v tail | grep -v grep | grep -v ibrun | grep -v ibend | awk '{ print $2 }'`

	if [ "${PID}" = "" ];
	then
		echo "[32mSTATUS : service [${SVC_NM}] is not running...[0m"
	else
		echo "#--------------------------------------------------------------------#"
		ps -ef | grep ${SVC_NM} | grep -v vi | grep -v tail | grep -v grep | grep -v xrun | grep -v xend
		# ps -ef | grep $1 | grep -v vi | grep -v tail | grep -v grep | grep -v xrun | grep -v xend | awk '{print $2}' | xargs kill
		# echo ${PID} | xargs kill    # signal handler Ã³¸®
#		kill -9 ${PID}              # Ä¡¸íÀûÀÎ ¾àÁ¡Àº signal handler Ã³¸®¸¦ ¸øÇÔ
		kill ${PID}              # Ä¡¸íÀûÀÎ ¾àÁ¡Àº signal handler Ã³¸®¸¦ ¸øÇÔ
		echo "[32mSTATUS : service [${SVC_NM}] stopped ...(sleep 1).[0m"
		echo "#--------------------------------------------------------------------#"
		sleep 1
	fi
}

#---------------------------------------------------------------------
# FUNCTION     display
#
display()
{
	# ±â°ü¸í Ãâ·Â
	case ${ORG_CD} in
		"TMP" ) echo "±â°ü¸í : Å×½ºÆ®±â°ü";;
		"KBB" ) echo "±â°ü¸í : (KBB) ±¹¹ÎÀºÇà        ";;
		"SHB" ) echo "±â°ü¸í : (SHB) ½ÅÇÑÀºÇà        ";;
		"WRB" ) echo "±â°ü¸í : (WRB) ¿ì¸®ÀºÇà        ";;
		"TKB" ) echo "±â°ü¸í : (TKB) ´ë±¸ÀºÇà        ";;
		"EXB" ) echo "±â°ü¸í : (EXB) ¿ÜÈ¯ÀºÇà        ";;
		"HNB" ) echo "±â°ü¸í : (HNB) ÇÏ³ªÀºÇà        ";;
		"PSB" ) echo "±â°ü¸í : (PSB) ºÎ»êÀºÇà        ";;
		"POS" ) echo "±â°ü¸í : (POS) ¿ìÃ¼±¹          ";;
		"NHB" ) echo "±â°ü¸í : (NHB) ³óÇù            ";;
		"GUB" ) echo "±â°ü¸í : (GUB) ±â¾÷ÀºÇà(x3)    ";;
		"SCB" ) echo "±â°ü¸í : (SCB) SCÁ¦ÀÏÀºÇà      ";;
		"SHC" ) echo "±â°ü¸í : (SHC) ½ÅÇÑÄ«µå        ";;
		"HDC" ) echo "±â°ü¸í : (HDC) Çö´ëÄ«µå        ";;
		"LTC" ) echo "±â°ü¸í : (LTC) ·Ôµ¥Ä«µå        ";;
		"SSC" ) echo "±â°ü¸í : (SSC) »ï¼ºÄ«µå        ";;
		"CTC" ) echo "±â°ü¸í : (CTC) ½ÃÆ¼Ä«µå        ";;
		"NHC" ) echo "±â°ü¸í : (NHC) ³óÇùÄ«µå        ";;
		"BCC" ) echo "±â°ü¸í : (BCC) BCÄ«µå          ";;
		"KBC" ) echo "±â°ü¸í : (KBC) ±¹¹ÎÄ«µå        ";;
		"DHI" ) echo "±â°ü¸í : (DHI) ´ëÇÑ»ý¸í        ";;
		"KIS" ) echo "±â°ü¸í : (KIS) ³ªÀÌ½º          ";;
		"KFT" ) echo "±â°ü¸í : (KFT) ±ÝÀ¶°áÁ¦¿ø      ";;
		"NCE" ) echo "±â°ü¸í : (NCE) NICE            ";;
		"HNT" ) echo "±â°ü¸í : (HNT) ÇÑ³×Æ®          ";;
		"GBK" ) echo "±â°ü¸í : (GBK) °ÔÀÌÆ®¹ðÅ©      ";;
