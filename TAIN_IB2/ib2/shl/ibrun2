# clear
case ${HOST_NAME} in
	"dbizap01" | "pbizap01" | "pbizap02" )
		echo "* ¼­¹ö¸í(${HOST_NAME})Àº <¾÷¹«°èAP> ¼­¹öÀÔ´Ï´Ù.."
		;;
	"dfep03" | "pfep03" | "pfep04" )
		echo "* ¼­¹ö¸í(${HOST_NAME})Àº <FEP¹ðÅ·> ¼­¹öÀÔ´Ï´Ù.."
		;;
	* )
		echo "[31mERROR : ¼­¹ö¸í(${HOST_NAME})´Â Áö¿øÇÏÁö ¾Ê½À´Ï´Ù....[0m"
		exit 1
		;;
esac

#---------------------------------------------------------------------
# ÇÁ·Î±×·¥À» ½ÇÇàÇÑ´Ù.
#
if [ $# -gt 0 ];
then
	# echo "$# is OK....[$*]"
	echo ""

	for SVC_NM in $*
	# for SVC_NM in OIRTMP5101
	do
		SVC_NM_LEN=`expr length "${SVC_NM}"`

		echo "[32m============================ ARGS=[${SVC_NM}] ===============================[0m"

		if [ ${SVC_NM_LEN} -eq 10 ];
		then

			#-------------------------------------------------

			SVC_NM=`echo ${SVC_NM} | tr 'a-z' 'A-Z'`
			SVC_CD=`expr substr ${SVC_NM} 1 3`
			# echo "STATUS : [SVC_NM=${SVC_NM}] [SVC_CD=${SVC_CD}]"

			#-------------------------------------------------

			case ${HOST_NAME} in
				#-------------------------------------------------
				"dbizap01" | "pbizap01" | "pbizap02" )           # AP¼­¹ö
					case ${SVC_CD} in
						"OFS" | "OFR" )
							#echo "STATUS : ¼­ºñ½º¸í(${SVC_NM})À» ½ÇÇàÇÕ´Ï´Ù..."
							execute ${SVC_NM} ${SVC_CD};
							echo "STATUS : RETURN CODE=$?"
							;;
						* )
							echo "[31mERROR : <¾÷¹«°èAP>¿¡´Â ¼­ºñ½ºÄÚµå(${SVC_CD})´Â Áö¿øÇÏÁö ¾Ê½À´Ï´Ù....[0m"
							;;
					esac
					;;
				#-------------------------------------------------
				"dfep03" | "pfep03" | "pfep04" )                 # FEP¼­¹ö
					case ${SVC_CD} in
						"OIS" | "OIR" | "OXS" | "OXR" | "OXA" | "OSA" | "BIS" | "BIR" | "BXS" | "BXR" | "IBN")
							#echo "STATUS : ¼­ºñ½º¸í(${SVC_NM})À» ½ÇÇàÇÕ´Ï´Ù..."
							execute ${SVC_NM} ${SVC_CD};
							echo "STATUS : RETURN CODE=$?"
							;;
						* )
							echo "[31mERROR : <FEP¹ðÅ·>¿¡´Â ¼­ºñ½ºÄÚµå(${SVC_CD})´Â Áö¿øÇÏÁö ¾Ê½À´Ï´Ù....[0m"
							;;
					esac
					;;
				#-------------------------------------------------
				* )
					echo "[31mERROR : ¼­¹ö¸í(${HOST_NAME})´Â Áö¿øÇÏÁö ¾Ê½À´Ï´Ù....[0m"
					;;
			esac
		else
			echo "[31mERROR : ¼­¹ö½º¸í(${SVC_NM})´Â Áö¿øÇÏÁö ¾Ê½À´Ï´Ù....[0m"
		fi

		echo ""

	done;

else
	echo ""
	echo "STATUS : argument count is $#...."
	echo "USAGE : ${0} [SERVICE_NAME]....."
	echo "       ex) ${0} OFSTMP0102 ofrTMP5101 OIRTTT0101 bxskbb0101 oirtmp0101 oiskbb0101 OXSTMP0101 BIRKBB5101 OFSKBB5101"
fi

